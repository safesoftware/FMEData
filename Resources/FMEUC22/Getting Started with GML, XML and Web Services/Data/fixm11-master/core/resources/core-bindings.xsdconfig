        <xb:package>aero.fixm.xmlbeans.fx</xb:package>
    </xb:namespace>

    <xb:namespace uri="http://www.fixm.aero/msg/1.0">
        <xb:package>aero.fixm.xmlbeans.msg</xb:package>
    </xb:namespace>
    
    <xb:namespace uri="http://www.fixm.aero/base/1.0">
        <xb:package>aero.fixm.xmlbeans.base</xb:package>
    </xb:namespace>
    
</xb:config>
